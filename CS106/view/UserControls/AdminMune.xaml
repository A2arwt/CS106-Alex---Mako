<UserControl x:Class="CS106.view.UserControls.AdminMune"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CS106.view.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Content="Manage Leave Request" FontSize="32" Click="ManageLeave"></Button>
            <Button Grid.Row="1" Content="Add to Roster" FontSize="32" Click="AddToRoster"></Button>
            <Button Grid.Row="2" Content="Edit Roster" FontSize="32" Click="EditRoster"></Button>
            <Button Grid.Row="3" Content="Edit Employee Details" FontSize="32" Click="EditEmployeeDetails" ></Button>
            <Button Grid.Row="5" Content="Logout" FontSize="32" ></Button>
        <Button Grid.Row="4" Content="Exit Employee Management" FontSize="32" Click="Exit"></Button>
    </Grid>
</UserControl>
